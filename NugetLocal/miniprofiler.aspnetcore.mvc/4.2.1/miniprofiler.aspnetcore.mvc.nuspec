<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>MiniProfiler.AspNetCore.Mvc</id>
    <version>4.2.1</version>
    <title>MiniProfiler for ASP.NET Core MVC</title>
    <authors>Nick Craver</authors>
    <owners>Nick Craver</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <projectUrl>https://miniprofiler.com/</projectUrl>
    <description>Lightweight mini-profiler, designed for ASP.NET Core MVC (*not* System.Web) websites</description>
    <releaseNotes>See https://miniprofiler.com/dotnet/Releases for more info</releaseNotes>
    <tags>ASP.NET Core MVC MVC Core Profiler Performance Profiling Timing</tags>
    <repository type="git" url="https://github.com/MiniProfiler/dotnet.git" branch="master" commit="b27bea37e9f32abbfec90918c0d747a78e1aeca6" />
    <dependencies>
